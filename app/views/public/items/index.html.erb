商品一覧

<div class="row justify-content-center">
　<% @item.each do |item| %>
　 <div class="one-post">
　　<p>画像</p>
　　<%= link_to item_path(@item) do %>
      <%= attachment_image_tag item, :image, :fill, 100, 100, format: 'jpeg' %>
　　<% end %>


　　<%= item.name %>


　　<%= item.price %>
   </div>
　<% end %>
</div>

