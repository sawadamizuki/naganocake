<div class="container">
<h3>会員一覧</h3>
 <div class="col-md-3"></div>
 <div class="col-md-6">
  <table class="table">
   <thead>
     <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
     </tr>
   </thead>
   <tbody>
    <% @customers.each do |customer| %>
     <tr>
       <td><%= customer.id%></td>
       <td>
          <%= link_to admin_customer_path(customer.id) do %>
           <%= customer.last_name %><%= customer.first_name %>
          <% end %>
       </td>
       <td><%= customer.email %></td>
       <% if customer.is_deleted == "Available" %>
         <td>有効</td>
        <% else %>
         <td>退会</td>
       <% end %>
     </tr>
    <% end %>
    <%= paginate @customers %>
   </tbody>
  </table>
 </div>
 <div class="col-md-3"></div>
</div>
