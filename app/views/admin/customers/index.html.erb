<div class="container">
<h3>会員一覧</h3>
 <div class="col-md-3"></div>
 <div class="col-md-6">
  <table class="table">
   <thead>
     <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
     </tr>
   </thead>
   <tbody>
    <% @customers.each do |customers| %>
     <tr>
       <td><%= customers.id%></td>
       <td>
          <%= link_to admin_customer_path(customers.id) do %>
           <%= customers.last_name %><%= customers.first_name %>
          <% end %>
       </td>
       <td><%= customers.email %></td>
       <td></td>
     </tr>
    <% end %>
    <%= paginate @customers %>
   </tbody>
  </table>
 </div>
 <div class="col-md-3"></div>
</div>
